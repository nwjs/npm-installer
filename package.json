  "files": [
    "lib",
    "bin",
    "scripts",
    "index.js"
  ],
  "author": "Kyle Robinson Young",
  "license": "MIT",
  "dependencies": {
    "chalk": "~1.1.3",
    "decompress": "^4.2.0",
    "download": "^5.0.3",
    "file-exists": "^2.0.0",
    "merge": "^2.1.1",
    "progress": "^2.0.3",
    "rimraf": "^2.2.8",
    "semver": "^5.1.0",
    "yargs": "^3.2.1"
  },
  "devDependencies": {
    "request": "^2.53.0"
  },
  "engines": {
    "node": ">= 20.5.0 || >= 18.18.0"
