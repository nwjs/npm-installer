{
  "name": "nw",
  "version": "0.80.0",
  "description": "A installer for nw.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/nwjs/npm-installer.git"
  },
  "main": "index.js",
  "bin": {
    "nw": "bin/nw"
  },
  "scripts": {
    "postinstall": "node scripts/install.js",
    "test": "node --test-reporter=spec --test test/index.mjs"
  },
  "files": [
    "lib",
    "bin",
    "scripts",
    "index.js"
  ],
  "author": {
    "name": "Kyle Robinson Young",
